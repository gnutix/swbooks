{
    "name": "gnutix/swbooks",
    "description": "This single web page application allows me to list my Star Wars books.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Application\\": "src/Application",
            "Gnutix\\": "src/Gnutix"
        }
    },
    "require": {
        "php": ">=7.4",
        "twig/twig": "~3.0",
        "symfony/http-kernel": "~5.0",
        "symfony/yaml": "~5.0",
        "symfony/config": "~5.0",
        "symfony/dependency-injection": "~5.0",
        "symfony/twig-bundle": "~5.0",
        "symfony/finder": "~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "squizlabs/php_codesniffer": "~2.0",
        "symfony/console": "~5.0",
        "symfony/process": "~5.0",
        "symfony/browser-kit": "~5.0",
        "symfony/css-selector": "~5.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable",
    "scripts": {
        "ci": [
            "@tests",
            "@cs"
        ],
        "cs": "php ecs.phar check --ansi",
        "cs-fix": "php ecs.phar check --ansi --fix",
        "tests": "bin/phpunit"
    }
}
